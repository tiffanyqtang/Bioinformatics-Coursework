      "text/plain": [
       "[1] 10"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    },
    {
     "data": {
      "text/html": [
       "100"
      ],
      "text/latex": [
       "100"
      ],
      "text/markdown": [
       "100"
      ],
      "text/plain": [
       "[1] 100"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# calculating lambda for each N\n",
    "\n",
    "G <- 10000\n",
    "L <- 100\n",
    "\n",
    "calculate_lambda <- function(N, L, G) {\n",
    "  if (G == 0) {\n",
    "    stop(\"G (genome size) cannot be zero.\")\n",
    "  }\n",
    "  Lambda <- (N * L) / G\n",
    "  return(Lambda)\n",
    "}\n",
    "\n",
    "#for N = 100\n",
    "n100_lambda <- calculate_lambda(100, L, G)\n",
    "n100_lambda\n",
    "#for N = 1,000\n",
    "n1000_lambda <- calculate_lambda(1000, L, G)\n",
    "n1000_lambda\n",
    "#for N = 10,000\n",
    "n10000_lambda <- calculate_lambda(10000,L, G)\n",
    "n10000_lambda"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
